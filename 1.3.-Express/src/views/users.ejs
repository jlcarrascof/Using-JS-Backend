<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>All Users</h1>
    <table>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Age</td>
        </tr>
        <tr>
            <% users.forEach((user) => { %>
                <td> <%= user.id %> </td>
                <td> <%= user.name %> </td>
                <td> <%= user.age %> </td>
            <% }) %>
        </tr>
    </table>
</body>
</html>
